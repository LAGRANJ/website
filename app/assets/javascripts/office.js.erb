$(function () {    
    $('.tree li > span').on('click', function (e) {
        deleteMarkers();
        $.ajax({
            <% url = WEBSite::Application.routes.url_helpers %>
                url: "<%= url.show_office_info_path()%>",
                method: 'POST',
                dataType: "script",
                data: {
                    officeid: $(this).data("officeid")
                },
                success: function(data, textStatus) {},
                error: function() {}
            });
    });
    
      
});