
<% commissionfee = GetCommissionFee(params["typeid"].to_i,params["summa"].to_i,params["currencyid"].to_i, params["countryid"].to_i)%>
<% if(commissionfee!=nil) %>	
	<% if !commissionfee.in_percent %>
		$('#commission_fee').val(<%=commissionfee.commission %>+' <%=GetCurrencySymbol(commissionfee.currencyid)%>')
		$('#commission_rate').val(<%=commissionfee.commission %> +' <%=GetCurrencySymbol(commissionfee.currencyid)%>')
	<% else %>
		<% commission_fee = commissionfee["Commission"]*params["summa"].to_i/100 %>		
		$('#commission_fee').val(<%=commission_fee %> +' <%=GetCurrencySymbol(commissionfee.currencyid)%>')
		$('#commission_rate').val(<%=commissionfee.commission %>+'%');
	<% end %>
<% else %>
	$('#commission_rate').val('тариф отсутствует')
	$('#commission_fee').val('не определен')
<% end %>